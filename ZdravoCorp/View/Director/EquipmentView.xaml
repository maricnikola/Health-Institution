<UserControl x:Class="ZdravoCorp.View.EquipmentView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:ZdravoCorp.View"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="700"
             Background="#29262e">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <StackPanel Margin="0,20, 0, 0" HorizontalAlignment="Center" Orientation="Horizontal">
            <TextBox Grid.Column="1" Width="180" Height="40" VerticalContentAlignment="Center" HorizontalAlignment="Left" Margin="5" Style="{DynamicResource TextBox}"></TextBox>

            <ComboBox Width="100" Margin="10,20,10, 10"></ComboBox>
            <ComboBox  Width="100" Margin="10,20,10, 10"></ComboBox>
            <ComboBox  Width="100" Margin="10,20,10, 10"> </ComboBox>
            <CheckBox Content="StockRoom" Foreground="White" Margin="10,20,10, 10"></CheckBox>
        </StackPanel>
    </Grid>
</UserControl>
